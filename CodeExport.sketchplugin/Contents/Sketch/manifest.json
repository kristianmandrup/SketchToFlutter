{
  "name": "Export Source Code ",
  "identifier": "com.tecla5.sketchToCode",
  "version": "0.0.1",
  "description": "Export platorm specific UI code directly from Sketch!",
  "authorEmail": "kmandrup@gmail.com",
  "author": "Kristian Mandrup",
  "commands": [{
      "script": "FlutterExport.js",
      "handler": "onRun",
      "shortcut": "command alt f",
      "name": "Copy Flutter Code",
      "identifier": "FlutterExport"
    },
    {
      "script": "iOsSwiftExport.js",
      "handler": "onRun",
      "shortcut": "command alt s",
      "name": "Copy iOS Swift UI Code",
      "identifier": "SwiftExport"
    },
    {
      "script": "AndroidKotlinExport.js",
      "handler": "onRun",
      "shortcut": "command alt k",
      "name": "Copy Android Kotlin UI Code",
      "identifier": "KotlinExport"
    },
    {
      "script": "PolymerExport.js",
      "handler": "onRun",
      "shortcut": "command alt p",
      "name": "Copy Polymer Code",
      "identifier": "PolymerExport"
    }
  ],
